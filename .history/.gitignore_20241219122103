# Ignore history files
.history/

# Ignore installation scripts
install*.sh

# Ignore Python bytecode files
__pycache__/
*.pyc
*.pyo
*.pyd

# Ignore virtual environment directories
.venv/
env/
venv/

# Ignore log files
*.log

# Ignore temporary files
*.tmp
*.temp

# Ignore IDE specific files
.vscode/
.idea/
*.swp
